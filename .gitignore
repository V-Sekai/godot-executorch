!thirdparty/executorch/examples/apple/coreml/executor_runner/coreml_executor_runner.xcworkspace/xcshareddata/
!thirdparty/executorch/examples/apple/coreml/executor_runner/coreml_executor_runner.xcworkspace/xcshareddata/IDEWorkspaceChecks.plist

!thirdparty/executorch/examples/demo-apps/react-native/rnllama/ios/rnllama.xcworkspace/
